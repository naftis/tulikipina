{
  "name": "gulp-template",
  "version": "0.0.0",
  "description": "Project template for gulp.js",
  "main": "gulpfile.js",
  "scripts": {
    "prepublish": "bower install",
    "start": "gulp",
    "build": "gulp build",
    "deploy": "gulp build && ./deploy.sh"
  },
  "keywords": [
    "gulp",
    "template"
  ],
  "author": "",
  "dependencies": {
    "coffee-script": "~1.7.1",
    "knockout": "~3.1.0"
  },
  "devDependencies": {
    "gulp-rename": "~0.2.1",
    "gulp-uglify": "~0.1.0",
    "gulp-util": "~2.2.9",
    "gulp-stylus": "0.0.9",
    "gulp-jade": "~0.3.0",
    "gulp-browserify": "~0.5.0",
    "gulp-minify-css": "~0.2.0",
    "coffeeify": "~0.5.2",
    "tiny-lr": "0.0.5",
    "gulp-livereload": "~0.2.0",
    "bower": "~1.2.8",
    "gulp": "~3.5.2",
    "browserify-shim": "~3.3.1",
    "gulp-plumber": "~0.5.6",
    "gulp-autoprefixer": "0.0.6",
    "express": "~3.5.0"
  },
  "browser": {
    "jquery": "./vendor/jquery/dist/jquery.js",
    "page.js": "./vendor/page.js/index.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "page.js": "page"
  },
  "browserify": {
    "transform": [
      "coffeeify",
      "browserify-shim"
    ]
  }
}
