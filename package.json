{
  "name": "gulp-template",
  "version": "0.0.0",
  "description": "Project template for gulp.js",
  "main": "gulpfile.js",
  "scripts": {
    "prepublish": "bower install",
    "start": "gulp",
    "build": "NODE_ENV=production gulp build"
  },
  "keywords": [
    "gulp",
    "template"
  ],
  "author": "",
  "dependencies": {
    "coffee-script": "~1.7.1",
    "instafeed.js": "^1.3.2",
    "knockout": "^3.3.0",
    "leaflet": "^0.7.5",
    "lodash": "~2.4.1",
    "marked": "~0.3.2",
    "timeago": "^0.2.0"
  },
  "devDependencies": {
    "bower": "^1.3.12",
    "browserify-plain-jade": "^0.2.2",
    "browserify-shim": "~3.3.1",
    "coffeeify": "~0.6.0",
    "event-stream": "~3.1.5",
    "express": "~3.5.0",
    "gulp": "~3.5.2",
    "gulp-autoprefixer": "0.0.6",
    "gulp-browserify": "~0.5.0",
    "gulp-combine-media-queries": "0.0.1",
    "gulp-concat": "~2.2.0",
    "gulp-cson": "0.0.1",
    "gulp-imagemin": "^0.5.0",
    "gulp-jade": "~0.3.0",
    "gulp-less": "~1.2.3",
    "gulp-livereload": "~0.2.0",
    "gulp-minify-css": "~0.3.3",
    "gulp-plumber": "~0.5.6",
    "gulp-rename": "~0.2.1",
    "gulp-replace": "~0.2.0",
    "gulp-rev": "~0.3.2",
    "gulp-streamify": "0.0.5",
    "gulp-stylus": "0.0.9",
    "gulp-uglify": "~0.1.0",
    "gulp-util": "~2.2.9",
    "rimraf": "~2.2.6",
    "tiny-lr": "0.0.5",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~0.8.2"
  },
  "browser": {
    "jquery": "./vendor/jquery/dist/jquery.js",
    "scrollTo": "./vendor/jquery.scrollTo/jquery.scrollTo.js",
    "modernizr": "./vendor/modernizr/modernizr.js",
    "routie": "./vendor/routie/dist/routie.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "modernizr": "Modernizr",
    "routie": "routie"
  },
  "browserify": {
    "transform": [
      "coffeeify",
      "browserify-plain-jade",
      "browserify-shim"
    ]
  }
}
