{
  "name": "gulp-template",
  "version": "0.0.0",
  "description": "Project template for gulp.js",
  "main": "gulpfile.js",
  "scripts": {
    "prepublish": "bower install",
    "start": "gulp",
    "build": "NODE_ENV=production gulp build",
    "deploy": "NODE_ENV=production gulp build && ./deploy.sh"
  },
  "keywords": [
    "gulp",
    "template"
  ],
  "author": "",
  "dependencies": {
    "coffee-script": "~1.7.1",
    "knockout": "~3.1.0"
  },
  "devDependencies": {
    "gulp-rename": "~0.2.1",
    "gulp-uglify": "~0.1.0",
    "gulp-util": "~2.2.9",
    "gulp-stylus": "0.0.9",
    "gulp-jade": "~0.3.0",
    "gulp-browserify": "~0.5.0",
    "gulp-minify-css": "~0.2.0",
    "tiny-lr": "0.0.5",
    "gulp-livereload": "~0.2.0",
    "bower": "~1.2.8",
    "gulp": "~3.5.2",
    "browserify-shim": "~3.3.1",
    "gulp-plumber": "~0.5.6",
    "gulp-autoprefixer": "0.0.6",
    "express": "~3.5.0",
    "markdown": "~0.5.0",
    "gulp-cson": "0.0.1",
    "coffeeify": "~0.6.0",
    "gulp-imagemin": "~0.2.0",
    "gulp-rev": "~0.3.2",
    "gulp-replace": "~0.2.0",
    "rimraf": "~2.2.6",
    "watchify": "~0.8.2",
    "vinyl-source-stream": "~0.1.1",
    "gulp-streamify": "0.0.5"
  },
  "browser": {
    "jquery": "./vendor/jquery/dist/jquery.js",
    "page.js": "./vendor/page.js/index.js",
    "gmaps": "./vendor/gmaps/gmaps.js",
    "scrollTo": "./vendor/jquery.scrollTo/jquery.scrollTo.js",
    "modernizr": "./vendor/modernizr/modernizr.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "page.js": "page",
    "gmaps": "GMaps",
    "modernizr": "Modernizr"
  },
  "browserify": {
    "transform": [
      "coffeeify",
      "browserify-shim"
    ]
  }
}
